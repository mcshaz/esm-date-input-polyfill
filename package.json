    "eslint": "^6.8.0",
    "httpster": "^1.0.4",
    "node-sass": "^4.13.1",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-delete": "^1.2.0",
